{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$defs": {
    "header": {
      "type": "object",
      "properties": {
        "msg_id": {
          "type": "string"
        },
        "session": {
          "type": "string"
        },
        "username": {
          "type": "string"
        },
        "date": {
        },
        "msg_type": {
          "type": "string"
        },
        "version": {
          "type": "string"
        }
      },
      "required": ["msg_id", "session", "username", "date", "msg_type", "version"],
      "additionalProperties": false
    },
    "error": {
      "type": "object",
      "properties": {
        "ename": {
          "type": "string"
        },
        "evalue": {
          "type": "string"
        },
        "traceback": {
          "type": "array",
          "items": {
            "type": "string"
          }
        }
      },
      "required": ["ename", "evalue", "traceback"]
    },
    "error_status": {
      "allOf": [{
        "$ref": "#/$defs/error"
      }, {
        "properties": {
          "status": {
            "const": "error"
          }
        },
        "required": ["status"]
      }]
    },
    "error_reply": {
      "allOf": [{
        "$ref": "#/$defs/error_status"
      }, {
        "additionalProperties": false
      }]
    }
  },
  "type": "object",
  "properties": {
    "header": {
      "$ref": "#/$defs/header"
    },
    "parent_header": {
      "oneOf": [{
        "$ref": "#/$defs/header"
      }, {
        "additionalProperties": false
      }]
    },
    "metadata": {
      "type": "object"
    },
    "content": {
      "type": "object"
    },
    "buffers": {
      "type": "array"
    },
    "msg_id": {
      "type": "string"
    },
    "msg_type": {
      "type": "string"
    }
  },
  "required": [
    "header", "parent_header", "metadata", "content", "buffers", "msg_id",
    "msg_type"
  ],
  "additionalProperties": false,
  "oneOf": [{
    "type": "object",
    "properties": {
      "msg_type": {
        "const": "clear_output"
      },
      "content": {
        "type" : "object",
        "properties": {
          "wait": {
            "type": "boolean"
          }
        },
        "required": ["wait"],
        "additionalProperties": false
      }
    }
  }, {
    "type": "object",
    "properties": {
      "msg_type": {
        "const": "comm_close"
      },
      "content": {
        "type" : "object",
        "properties": {
          "comm_id": {
            "type": "string"
          },
          "data": {
            "type": "object"
          }
        },
        "required": ["comm_id", "data"],
        "additionalProperties": false
      }
    }
  }, {
    "type": "object",
    "properties": {
      "msg_type": {
        "const": "comm_info_reply"
      },
      "content": {
        "type" : "object",
        "oneOf": [{
          "properties": {
            "status": {
              "const": "ok"
            },
            "comms": {
              "type": "object",
              "additionalProperties": {
                "type": "object",
                "properties": {
                  "target_name": {
                    "type": "string"
                  }
                },
                "required": ["target_name"],
                "additionalProperties": false
              }
            }
          },
          "required": ["status", "comms"],
          "additionalProperties": false
        }, {
          "$ref": "#/$defs/error_status"
        }]
      }
    }
  }, {
    "type": "object",
    "properties": {
      "msg_type": {
        "const": "comm_msg"
      },
      "content": {
        "type" : "object",
        "properties": {
          "comm_id": {
            "type": "string"
          },
          "data": {
            "type": "object"
          }
        },
        "required": ["comm_id", "data"],
        "additionalProperties": false
      }
    }
  }, {
    "type": "object",
    "required": ["msg_type", "content"],
    "properties": {
      "msg_type": {
        "const": "comm_open"
      },
      "content": {
        "type" : "object",
        "properties": {
          "comm_id": {
            "type": "string"
          },
          "target_name": {
            "type": "string"
          },
          "data": {
            "type": "object"
          }
        },
        "required": ["comm_id", "target_name", "data"],
        "additionalProperties": false
      }
    }
  }, {
    "type": "object",
    "properties": {
      "msg_type": {
        "const": "complete_reply"
      },
      "content": {
        "oneOf": [{
          "type" : "object",
          "properties": {
            "status": {
              "const": "ok"
            },
            "matches": {
              "type": "array",
              "items": {
                "type": "string"
              }
            },
            "cursor_start": {
              "type": "integer"
            },
            "cursor_end": {
              "type": "integer"
            },
            "metadata": {
              "type": "object"
            }
          },
          "required": ["status", "matches", "cursor_start", "cursor_end", "metadata"],
          "additionalProperties": false
        }, {
          "$ref": "#/$defs/error_status"
        }]
      }
    }
  }, {
    "type": "object",
    "properties": {
      "msg_type": {
        "enum": ["display_data", "update_display_data"]
      },
      "content": {
        "type": "object",
        "properties": {
          "data": {
            "type": "object"
          },
          "metadata": {
            "type": "object"
          },
          "transient": {
            "type": "object"
          }
        },
        "required": ["data", "metadata"],
        "additionalProperties": false
      }
    }
  }, {
    "type": "object",
    "required": ["msg_type", "content"],
    "properties": {
      "msg_type": {
        "const": "error"
      },
      "content": {
        "type": "object",
        "$ref": "#/$defs/error"
      }
    }
  }, {
    "type": "object",
    "properties": {
      "msg_type": {
        "const": "execute_input"
      },
      "content": {
        "type": "object",
        "properties": {
          "code": {
            "type": "string"
          },
          "execution_count": {
            "type": "integer"
          }
        },
        "required": ["code", "execution_count"],
        "additionalProperties": false
      }
    }
  }, {
    "type": "object",
    "required": ["msg_type", "content"],
    "properties": {
      "msg_type": {
        "const": "execute_reply"
      },
      "content": {
        "oneOf": [{
          "type" : "object",
          "properties": {
            "status": {
              "const": "ok"
            },
            "execution_count": {
              "type": "integer"
            },
            "payload": {
              "type": "array",
              "items": {
                "oneOf": [{
                  "type": "object",
                  "properties": {
                    "source": {
                      "const": "page"
                    },
                    "data": {
                      "type": "object"
                    },
                    "start": {
                      "type": "integer"
                    }
                  },
                  "required": ["source", "data", "start"],
                  "additionalProperties": false
                }, {
                  "type": "object",
                  "properties": {
                    "source": {
                      "const": "set_next_input"
                    },
                    "text": {
                      "type": "string"
                    },
                    "replace": {
                      "type": "boolean"
                    }
                  },
                  "required": ["source", "text", "replace"],
                  "additionalProperties": false
                }, {
                  "type": "object",
                  "properties": {
                    "source": {
                      "const": "edit"
                    },
                    "filename": {
                      "type": "string"
                    },
                    "line_number": {
                      "type": "integer"
                    }
                  },
                  "required": ["source", "filename", "line_number"],
                  "additionalProperties": false
                }, {
                  "type": "object",
                  "properties": {
                    "source": {
                      "const": "ask_exit"
                    },
                    "keepkernel": {
                      "type": "boolean"
                    }
                  },
                  "required": ["source", "keepkernel"],
                  "additionalProperties": false
                }]
              }
            },
            "user_expressions": {
              "type": "object"
            }
          },
          "required": ["status", "execution_count"],
          "additionalProperties": false
        }, {
          "allOf": [{
            "$ref": "#/$defs/error_status"
          }, {
            "properties": {
              "execution_count": {
                "type": "integer"
             }
            },
            "required": ["execution_count"]
          }]
        }]
      }
    }
  }, {
    "type": "object",
    "properties": {
      "msg_type": {
        "const": "execute_result"
      },
      "content": {
        "type": "object",
        "properties": {
          "data": {
            "type": "object"
          },
          "metadata": {
            "type": "object"
          },
          "execution_count": {
            "type": "integer"
          }
        },
        "required": ["data", "metadata", "execution_count"],
        "additionalProperties": false
      }
    }
  }, {
    "type": "object",
    "properties": {
      "msg_type": {
        "const": "history_reply"
      },
      "content": {
        "oneOf": [{
          "type" : "object",
          "properties": {
            "status": {
              "const": "ok"
            },
            "history": {
              "type": "array",
              "items": {
                "type": "array",
                "items": [{
                  "type": "string"
                }, {
                  "type": "string"
                }, {
                  "anyOf": [{
                    "type": "string"
                  }, {
                    "type": "array",
                    "items": [{
                      "type": "string"
                    }, {
                      "type": "string"
                    }]
                  }]
                }]
              }
            }
          },
          "required": ["status", "history"],
          "additionalProperties": false
        }, {
          "$ref": "#/$defs/error_status"
        }]
      }
    }
  }, {
    "type": "object",
    "properties": {
      "msg_type": {
        "const": "input_reply"
      },
      "content": {
        "oneOf": [{
          "type" : "object",
          "properties": {
            "status": {
              "const": "ok"
            },
            "value": {
              "type": "string"
            }
          },
          "required": ["value"],
          "additionalProperties": false
        }, {
          "$ref": "#/$defs/error_status"
        }]
      }
    }
  }, {
    "type": "object",
    "properties": {
      "msg_type": {
        "const": "inspect_reply"
      },
      "content": {
        "oneOf": [{
        "type" : "object",
          "properties": {
            "status": {
              "const": "ok"
            },
            "found": {
              "type": "boolean"
            },
            "data": {
              "type": "object"
            },
            "metadata": {
              "type": "object"
            }
          },
          "required": ["found", "data", "metadata"],
          "additionalProperties": false
        }, {
          "$ref": "#/$defs/error_status"
        }]
      }
    }
  }, {
    "type": "object",
    "properties": {
      "msg_type": {
        "const": "interrupt_reply"
      },
      "content": {
        "oneOf": [{
          "type" : "object",
          "properties": {
            "status": {
              "const": "ok"
            }
          },
          "additionalProperties": false
        }, {
          "$ref": "#/$defs/error_status"
        }]
      }
    }
  }, {
    "type": "object",
    "properties": {
      "msg_type": {
        "const": "is_complete_reply"
      },
      "content": {
        "oneOf": [{
          "type": "object",
          "properties": {
            "status": {
              "const": "incomplete"
            },
            "indent": {
              "type": "string"
            }
          },
          "required": ["status", "indent"],
          "additionalProperties": false
        }, {
          "type": "object",
          "properties": {
            "status": {
              "enum": ["complete", "invalid", "unknown"]
            }
          },
          "required": ["status"],
          "additionalProperties": false
        }]
      }
    }
  }, {
    "type": "object",
    "properties": {
      "msg_type": {
        "const": "kernel_info_reply"
      },
      "parent_header": {
        "type": "object",
        "properties": {
          "msg_type": {
            "const": "kernel_info_request"
          }
        }
      },
      "content": {
        "type": "object",
        "oneOf": [{
          "properties": {
            "status": {
              "const": "ok"
            },
            "protocol_version": {
              "type": "string"
            },
            "implementation": {
              "type": "string"
            },
            "implementation_version": {
              "type": "string"
            },
            "language_info": {
              "type": "object",
              "properties": {
                "name": {
                  "type": "string"
                },
                "version": {
                  "type": "string"
                },
                "mimetype": {
                  "type": "string"
                },
                "file_extension": {
                  "type": "string"
                },
                "pygments_lexer": {
                  "type": "string"
                },
                "codemirror_mode": {
                  "anyOf": [{
                    "type": "string"
                  }, {
                    "type": "object"
                  }]
                },
                "nbconvert_exporter": {
                  "type": "string"
                }
              },
              "required": ["name", "version", "mimetype", "file_extension"],
              "additionalProperties": false
            },
            "banner": {
              "type": "string"
            },
            "help_links": {
              "type": "array",
              "items": {
                "type": "object",
                "properties": {
                  "text": {
                    "type": "string"
                  },
                  "url": {
                    "type": "string"
                  }
                },
                "required": ["text", "url"],
                "additionalProperties": false
              }
            }
          },
          "required": [
            "status", "protocol_version", "implementation",
            "implementation_version", "language_info", "banner"
          ],
          "additionalProperties": false
        }, {
          "$ref": "#/$defs/error_status"
        }]
      }
    }
  }, {
    "type": "object",
    "properties": {
      "msg_type": {
        "const": "shutdown_reply"
      },
      "content": {
        "oneOf": [{
          "type" : "object",
          "properties": {
            "status": {
              "const": "ok"
            },
            "restart": {
              "type": "boolean"
            }
          },
          "required": ["status", "restart"],
          "additionalProperties": false
        }, {
          "$ref": "#/$defs/error_status"
        }]
      }
    }
  }, {
    "type": "object",
    "properties": {
      "msg_type": {
        "const": "status"
      },
      "content": {
        "type" : "object",
        "properties": {
          "execution_state": {
            "enum": ["busy", "idle", "starting"]
          }
        },
        "required": ["execution_state"],
        "additionalProperties": false
      }
    }
  }, {
    "type": "object",
    "properties": {
      "msg_type": {
        "const": "stream"
      },
      "content": {
        "type" : "object",
        "properties": {
          "name": {
            "enum": ["stdout", "stderr"]
          },
          "text": {
            "type": "string"
          }
        },
        "required": ["name", "text"],
        "additionalProperties": false
      }
    }
  }]
}
